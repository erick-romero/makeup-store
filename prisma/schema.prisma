// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./makeup.db"
}

model Usuario {
  id    Int     @id @default(autoincrement())
  nombre String
  apellido String
  correo_electronico String  @unique
  telefono  String
  contrasena String
}

model Tipo_Usuario{
  id    Int     @id @default(autoincrement())
  nombre String
  Descripcion String
}

